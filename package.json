  "description": "Simple flow library",
  "main": "dist/drawflow.js",
  "exports": "./dist/drawflow.js",
  "types": "./dist/drawflow.d.ts",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:js && npm run build:css && npm run build:types",
    "build:js": "node ./scripts/build.js",
